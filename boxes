touch .box
echo      "+--------------+
          /|             /|
         / |            / |
        *--+-----------*  |
        |  |           |  |
        |  |           |  |
        |  |           |  |
        |  +-----------+--+
        | /            | /
        |/             |/
        *--------------*" >> .box
        echo "alias box='cat .box'" >> .bashrc
        
      
        
